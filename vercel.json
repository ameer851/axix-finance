{
  "version": 2,
  "buildCommand": "node vercel-build-helper.js && npm run build",
  "outputDirectory": "dist/public",
  "installCommand": "npm install --legacy-peer-deps",
  "functions": {
    "api/**/*": {
      "runtime": "@vercel/node@2.15.3"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/server"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
